{
  "name": "vtuber-db",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "dependencies": {
    "express": "^4.17.1",
    "googleapis": "^87.0.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "dev": "ts-node-dev src/server.ts"
  },
  "author": "Deemo Harlos",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "semi": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars-experimental": "warn"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
